<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ https://raw.githubusercontent.com/AcidWeb/wow-ui-source/live/Interface/FrameXML/UI.xsd">

    <Frame name="LibSettingsBuilder_EmbedCanvasTemplate" inherits="SettingsListElementTemplate" mixin="LibSettingsBuilder_EmbedCanvasMixin" virtual="true">
        <Scripts>
            <OnLoad method="OnLoad"/>
        </Scripts>
    </Frame>

    <Frame name="LibSettingsBuilder_SubheaderTemplate" mixin="LibSettingsBuilder_SubheaderMixin" virtual="true">
        <Size x="30" y="35"/>
        <Layers>
            <Layer level="OVERLAY">
                <FontString parentKey="Title" inherits="GameFontHighlightSmall" justifyH="LEFT" justifyV="TOP">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="35" y="-12"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>

    <Button name="LibSettingsBuilder_ScrollDropdownTemplate"
        inherits="SettingsDropdownControlTemplate"
        mixin="LibSettingsBuilder_ScrollDropdownMixin"
        virtual="true">
    </Button>

    <Script file="LibSettingsBuilder.lua"/>
</Ui>
